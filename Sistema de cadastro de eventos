//------------------------------------------------------------------------------------------------------------
var idade = 17
var data_limite = 31092021
var quantpess = 101
//------------------------------------------------------------------------------------------------------------

    console.log('--------------------------------------------------');
    console.log('insira sua idade: ');
    console.log(idade);
    console.log('-------------------------');

if(idade >= 18){
    console.log('OK');
    console.log('--------------------------------------------------');
}else{
    console.log('Nâo foi possivel efetuar o cadastro por restrição de idade.')
    console.log('--------------------------------------------------');
}

//------------------------------------------------------------------------------------------------------------
    console.log('--------------------------------------------------');
    console.log('insira a data limite para o evento: ');
    console.log(data_limite);
    console.log('-------------------------');

if(data_limite < 30092021){
    console.log('OK');
    console.log('--------------------------------------------------');
}else{
    console.log('Data limite excedida.')
    console.log('--------------------------------------------------');
}

//-----------------------------------------------------------------------------------------------------------
    console.log('--------------------------------------------------');
    console.log('insira o limite de pessoas para o evento: ');
    console.log(quantpess);
    console.log('-------------------------');

if(quantpess < 100){
    console.log('OK');
    console.log('--------------------------------------------------');
}else{
    console.log('quantidade excedida.')
    console.log('--------------------------------------------------');
}
